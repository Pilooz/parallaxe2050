    }
    #whichStep.waiting * {
        opacity: 0;
    }
    h1 {
        transition: all 0.2s linear;
        text-align: center;
        font-size: 80px;
    }
    #container {
        position: fixed;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        display: flex;
        flex-direction: column;
        align-items: stretch;
        color: white;
        padding: 60px 0;
        overflow-y: scroll;
        overflow-x: hidden;
    }
    div#firstContainer {
        position: relative;
        z-index: 80;
        flex: 1 1 auto;
        transition: all 0.5s linear;
    }

    div#firstContainer > button {
        position: absolute;
        z-index: 9;
        top: 50%;
        background: radial-gradient(rgba(255,255,255,0.2), rgba(255,255,255,0.2),rgba(255,255,255,0.4));
        border: none;
        border-radius: 50%;
        line-height: 20px;
        height: 60px;
        width: 60px;
        margin-top: -30px;
        font-size: 28px;
        color: white;
        font-family: 'Varela Round', sans-serif;
        opacity: 1;
    }
    div#firstContainer > button#previous {
        left: 50px;
    }
    div#firstContainer > button#next {
        right: 50px;
    }

    div#firstContainer > div#waiting {
        transition: all linear 0.5s;
        opacity: 0;
        position: absolute;
        top: 50%;
        left: 50%;
        width: 500px;
        height: 60px;
        margin-top: -30px;
        margin-left: -250px;
        z-index: 20;
        font-size: 28px;
        text-align: center;
    }
    div#sectionsContainer {
        transition: all linear 0.2s;
        width: 500%;
        display: flex;
        flex-wrap: nowrap;
        align-items: center;
        height: 100%;
        transform: translate(0px, 0px);
        opacity: 1;
    }
    div#sectionsContainer > section {
        flex: 1 1 20%;
        position: relative;
        z-index: 8;
        padding-left: 30px;
        padding-right: 30px;
    }
    div#sectionsContainer > section > div {
        translate: 0.5s linear all;
        background: radial-gradient(rgba(255,255,255,0.2), rgba(255,255,255,0.2), rgba(255,255,255,0.2),rgba(255,255,255,0.2),rgba(255,255,255,0.2),rgba(255,255,255,0.4));
        margin: auto;
        max-width: 800px;
        padding: 50px;
        border-radius: 5px;
        backdrop-filter: blur(4px);
        box-shadow: 0 0 20px rgba(0,0,0,0.1);
    }
    div#sectionsContainer > section > div > button,
    div#sectionsContainer > section > div > select,
    div#sectionsContainer > section > div > input {
        -webkit-appearance: none;
        transition: all 0.2s linear;
        width: 28%;
        padding: 20px;
        border-radius: 5px;
        outline: none;
        border: 1px solid rgba(255,255,255,0.5);
        background: none;
        color: white;
        font-family: 'Varela Round', sans-serif;
        font-size: 20px;
    }
    div#sectionsContainer > section > div > button {
        padding: 20px 40px;
        margin-left: 2%;
        width: 68%;
    }
    div#sectionsContainer > section > div > select option {
        background: none;
        -webkit-appearance: none;
    }
    div#sectionsContainer > section > div > button:hover {
        background: rgba(255,255,255,0.3);
        cursor: pointer;
    }
    label {
        display: block;
        margin-bottom: 30px;
        font-size: 50px;
    }
    label ~ p {
        padding-bottom: 60px;
        font-size: 26px;
        line-height: 30px;
        color: rgba(255,255,255,0.8);
    }
    select {
        appearance: none;
    }

    #whichStep.step-codeetprog div#sectionsContainer {
        transform: translate(0%, 0);
    }
    #whichStep.step-codeetprog button#previous {
        opacity: 0;
    }
    #whichStep.step-hardware div#sectionsContainer {
        transform: translate(-25%, 0);
    }
    #whichStep.step-bdd div#sectionsContainer {
        transform: translate(-50%, 0);
    }
    #whichStep.step-adminreseau div#sectionsContainer {
        transform: translate(-75%, 0);
    }
    #whichStep.step-adminreseau button#next {
        opacity: 0;
    }

    #finalActions {
        text-align: center;
    }
    #finalActions button {
        -webkit-appearance: none;
        transition: all 0.2s linear;
        width: 35%;
        padding: 20px;
        border-radius: 5px;
        outline: none;
        border: 1px solid rgba(255,255,255,0.5);
        background: none;
        color: white;
        font-family: 'Varela Round', sans-serif;
        font-size: 20px;
        background: radial-gradient(rgba(255,255,255,0.2), rgba(255,255,255,0.2), rgba(255,255,255,0.2),rgba(255,255,255,0.2),rgba(255,255,255,0.2),rgba(255,255,255,0.4));
        backdrop-filter: blur(4px);
        cursor: pointer;
