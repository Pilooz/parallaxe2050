<script>
$(document).ready(function() {

    // Paramatréage
    var rightAnswerCodeetprog = "Python", rightAnswerHardware = "!", rightAnswerBdd = "3 AM", rightAnswerAdminreseau = "4C";
    var answerCodeetprog, answerHardware, answerBdd, answerAdminreseau;
    
    // Passage d'une étape à l'autre
    var nextStep;
    var section;
    $('section button').on('click', function(e) {
        section = $(this).parents('section');
        switch(section.data('name')) {
            case 'codeetprog':
                answerCodeetprog = $('#codeetprog input').val();
                nextStep = "hardware";
                // $('#hardware input').focus();
                break;
            case 'hardware':
                answerHardware = $('#hardware input').val();
                nextStep = "bdd";
                break;
            case 'bdd':
                answerBdd = $('#bdd input').val();
                nextStep = "adminreseau";
                break;
            case 'adminreseau':
                answerAdminreseau = $('#adminreseau input').val();
                $('#validate').removeClass('disabled');
                break;
        }
        if(nextStep != "") {
            $('#whichStep').removeClass().addClass('step-' + nextStep);
            console.log(nextStep);
        }
    });


    // Redémarrage de la page
    $('#restart').on('click', function(e) {
        e.preventDefault();
        location.reload(true);
    })

    // Fake select
    $('#codeetprog input').on('click', function(e) {
        e.preventDefault();
        $('.fake_select.fake_select_codeetprog').addClass('show');
    })
    $('#bdd input').on('click', function(e) {
        e.preventDefault();
        $('.fake_select.fake_select_bdd').addClass('show');
    })
    $('.fake_select li').on('click', function(e) {
        e.preventDefault();
        $('.fake_select').removeClass('show');
        if($(this).parents('.fake_select').hasClass('fake_select_codeetprog')) {
            $('#codeetprog input').val($(this).html());
        }
        if($(this).parents('.fake_select').hasClass('fake_select_bdd')) {
            $('#bdd input').val($(this).html());
        }
    })



    // Bouton de validation
    $('#validate').on('click', function(e) {
        e.preventDefault();
        // console.log("rightAnswerCodeetprog", rightAnswerCodeetprog);
        // console.log("answerCodeetprog", answerCodeetprog);
        // console.log("rightAnswerHardware", rightAnswerHardware);
        // console.log("answerHardware", answerHardware);
        // console.log("rightAnswerBdd", rightAnswerBdd);
        // console.log("answerBdd", answerBdd);
        // console.log("rightAnswerAdminreseau", rightAnswerAdminreseau);
        // console.log("answerAdminreseau", answerAdminreseau);
        var success = "0";
        if(
            (answerCodeetprog == "Choisir" || answerCodeetprog == "Aucune idée" || answerCodeetprog == rightAnswerCodeetprog) &&
            (answerHardware == "" || answerHardware == rightAnswerHardware) &&
            (answerBdd == "Choisir" || answerBdd == "Aucune idée" || answerBdd == rightAnswerBdd) &&
            (answerAdminreseau == "" || answerAdminreseau == rightAnswerAdminreseau) &&
            (answerCodeetprog == rightAnswerCodeetprog || answerHardware == rightAnswerHardware || answerBdd == rightAnswerBdd || answerAdminreseau == rightAnswerAdminreseau)
            ) {
            success = "1";
        }
        $('#whichStep').addClass('waiting');
        setTimeout(function(){ window.location = "lancement-anti-virus?s=" + success; }, 200);
    })
});
</script>
