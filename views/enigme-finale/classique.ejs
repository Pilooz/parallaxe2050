    <style type="text/css">
/* Actions et boutons */
  body #actions {
    z-index: 980 !important; }
    body #actions .button:hover, body #actions .btn:hover {
      box-shadow: 0 0px 20px #00c6f9; }
#actions {
  position: fixed;
  bottom: 20px;
  right: 20px;
  display: flex;
  flex-direction: row; }
  #actions button,
  #actions .button, #actions .btn {
    right: 20px;
    margin-left: 20px;
    padding: 0.375rem 0.75rem;
    background-color: #00c6f9;
    transition: color 0.15s ease-in-out,background-color 0.15s ease-in-out,border-color 0.15s ease-in-out,box-shadow 0.15s ease-in-out;
    border: 1px solid transparent;
    border-radius: 0.25rem;
    line-height: 1.5;
    font-size: 1rem;
    font-weight: 400;
    color: white; }
    #actions button:hover,
    #actions .button:hover, #actions .btn:hover {
      box-shadow: 0 0 10px #00c6f9; }
    #actions button:focus,
    #actions .button:focus, #actions .btn:focus {
      outline: none; }

/* Thumbnail de vidéo pour relancer la vidéo précédente */
#thumbnail {
  position: fixed;
  z-index: 90;
  left: 20px;
  bottom: 20px;
  transition: all 0.4s ease-in;
  cursor: pointer;
  transform-origin: bottom left; }
  #thumbnail.small {
    transform: scale(0.1);
    transform-origin: bottom left; }
  body #player {
    display: flex;
    position: fixed;
    z-index: 900;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    align-items: center;
    justify-content: center;
    margin: 0;
    background-color: #000000 !important;
    transition: all 0.4s ease-in;
    transform-origin: bottom left; }
    body #player .module-media-video {
      margin: 0 !important;
      background-color: #000000 !important; }
      body #player .module-media-video .module-media-video-inner {
        background-color: #000000 !important; }
        body #player .module-media-video .module-media-video-inner video,
        body #player .module-media-video .module-media-video-inner video:focus {
          outline: none;
          background-color: #000000 !important; }
  body.thumbnail {
    background: #303030; }
    body.thumbnail #actions {
      opacity: 0;
      pointer-events: none; }
      body.thumbnail #actions button {
        pointer-events: none; }
    body.thumbnail #player {
      transform: scale(0.1);
      transform-origin: bottom left;
      left: 20px;
      bottom: 20px;
      cursor: pointer; }
      body.thumbnail #player video {
        pointer-events: none; }

